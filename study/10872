import java.util.*;

public class Main {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);

        int n = in.nextInt();
        int result = 1;
        if (n == 0) result = 1;
        else {
            for (int i = n; i >= 1; i--) {
                result *= i;
            }
        }
        System.out.println(result);
    }
}
