import java.util.*;

class Main {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);

        int N = in.nextInt();

        int room = 1;
        int count = 1;

        while (room < N){
            count ++;
            room += 6 * count;
        }
        System.out.println(count);

    }
}
