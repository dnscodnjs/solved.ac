import java.util.Scanner;
//알파벳에서 해당 문자열 인덱스 표시하기
class Main {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);

        String s = in.nextLine();

        for (char c = 'a'; c <= 'z'; c++){
            System.out.print(s.indexOf(c)+ " ");
        }

    }

}
